{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    // add parameters like below
    // /dev/ttyXR0 response_data 2000000 O 8 1 104857600 csv 1 0f 22 10 1e 22 10
    "configurations": [
        {
            "name": "Launch Program",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/c_backend/command_response",
            "args": [
                "/dev/ttyXR0",
                "response_data",
                "2000000",
                "O",
                "8",
                "1",
                "104857600",
                "csv",
                "1",
                "0f",
                "22",
                "10",
                "1e",
                "22",
                "10"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}